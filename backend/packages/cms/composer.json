{
    "name": "user/laravel-cms",
    "description": "A reusable Laravel package providing core CMS functionalities for multiple projects",
    "type": "library",
    "license": "MIT",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "version": "1.0.0",
    "require": {},
    "autoload": {
        "psr-4": {
            "User\\LaravelCms\\": "src/",
            "User\\Database\\Seeders\\": "database/seeders/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "User\\LaravelCms\\Providers\\CmsServiceProvider"
            ]
        }
    }
}
